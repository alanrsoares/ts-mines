{
  "name": "ts-mines",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prebuild": "yarn generate-build-version && yarn write-app-name-to-manifest",
    "generate-build-version": "node scripts/generate-build-version",
    "write-app-name-to-manifest": "node scripts/write-app-name-to-manifest",
    "postversion": "git push && git push --tags",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.2.0",
    "polished": "^3.6.3",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.0",
    "typescript": "~3.9.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/ramda": "^0.27.4",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "unstated-next": "^1.1.0"
  }
