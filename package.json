    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^13.13.52",
    "@types/ramda": "^0.27.46",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "esbuild-loader": "^2.16.0",
